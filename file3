pipeline{
  agent any

  stages{
    stage('Checkout'){
      steps{
        git branch:'main',url:"https://github.com/1ms24mc087/jenkins"
      }
    }
    stage('Build'){
      steps{
        echo "Developing numbers from 1-10"
      } 
    }
    
    stage('Executing file'){
      steps{
        sh "python3 numbers.py"
      } 
    }
  }
}
